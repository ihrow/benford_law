generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_PRISMA_URL")
}

model digits {
  id       Int      @id @default(autoincrement())
  updateAt DateTime @updatedAt
  one      Int
  two      Int
  three    Int
  four     Int
  five     Int
  six      Int
  seven    Int
  eight    Int
  nine     Int
}

model crypto {
  id       Int      @id @default(autoincrement())
  updateAt DateTime @updatedAt
  name     String
  price    Float
}
